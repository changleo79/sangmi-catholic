{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/upload.ts": {
      "maxDuration": 60,
      "memory": 3008,
      "maxRequestBodySize": "50mb"
    }
  }
}
